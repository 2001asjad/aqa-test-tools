<benchmarks>
	<benchmark name="Canary" variant="end" setup="true">
		<capabilities/>
		<properties>
			<property name="benchmarkArgs"></property>
			<property name="timeout">300000</property>
			<property name="minHeapSize">512</property>
			<property name="maxHeapSize">512</property>
			<property name="JDKOptions">-Xint -Dfile={perffarm_root}/canary_data/canary.out</property>
			<property name="environment"></property>
			<property name="workDir">{perffarm_root}/Canary</property>
			<property name="resultTag">nominal</property>
		</properties>	
		<iterations>1</iterations>
	</benchmark>
</benchmarks>